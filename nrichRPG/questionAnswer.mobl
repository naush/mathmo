module nrichRPG::questionAnswer

entity QA {
	number	: Num  // position in list
	topic : String // "partialFraction" ...
	question : String
	answer : String
	done : Bool
	correct : Bool
	plotData : String
	questionHtml : String
	answerHtml : String
	plotHtml : String
}

entity TopicScore {
	topic: String
	agree:Num
	disagree:Num
}